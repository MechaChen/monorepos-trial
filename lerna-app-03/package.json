{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:frontend": "lerna run start --scope=@monorepos/ui",
    "start:backend": "lerna run start --scope=@monorepos/api",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\""
  },
  "dependencies": {
    "concurrently": "^9.2.1"
  },
  "devDependencies": {
    "lerna": "^9.0.3"
  }
}
